<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gr.zyxt.exodus">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/ic_robot_72"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_robot_72"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SettingsActivity"></activity>
        <activity android:name=".activity.MainControlsActivity" />
        <activity android:name=".activity.onboarding.WelcomeActivity" />
        <activity android:name=".activity.onboarding.EnterAddressActivity" />
        <activity android:name=".activity.onboarding.EnterFullNameActivity" />
        <activity android:name=".activity.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>